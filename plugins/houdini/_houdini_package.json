{
    "path": [
        "$FXQUINOX",
        "$FXQUINOX_HOUDINI_PLUGIN"
    ],
    "load_package_once": true,
    "enable": true,
    "version": "0.0.1",
    "env": [
        {
            "FXQUINOX": [
                {
                    "houdini_os == 'windows'": "$SERVER_ROOT/Projects/Code/fxquinox"
                },
                {
                    "houdini_os == 'linux'": ""
                },
                {
                    "houdini_os == 'macos'": ""
                }
            ]
        },
        {
            "FXQUINOX_PLUGINS": "$FXQUINOX/plugins"
        },
        {
            "FXQUINOX_HOUDINI_PLUGIN": "$FXQUINOX_PLUGINS/houdini"
        },
        {
            "FXQUINOX_USD_PLUGINS": "$FXQUINOX_PLUGINS/usd"
        },
        {
            "RESOLVER_NAME": "cachedResolver"
        },
        {
            "USD_ASSET_RESOLVER": "$FXQUINOX_USD_PLUGINS/usdAssetResolver"
        },
        {
            "PYTHONPATH": [
                "$FXQUINOX",
                {
                    "method": "append"
                },
                "$FXQUINOX_USD_PLUGINS/usdAssetResolver/$RESOLVER_NAME/lib/python",
                {
                    "method": "append"
                }
            ]
        },
        {
            "PXR_PLUGIN_PATH": [
                "$FXQUINOX_USD_PLUGINS/usdFxquinox/resources",
                {
                    "method": "append"
                },
                "$FXQUINOX_USD_PLUGINS/usdAssetResolver/$RESOLVER_NAME/resources",
                {
                    "method": "append"
                }
            ]
        },
        {
            "PATH": [
                "$FXQUINOX_USD_PLUGINS/usdAssetResolver/$RESOLVER_NAME/lib",
                {
                    "method": "append"
                }
            ]
        },
        {
            "TF_DEBUG": "AR_RESOLVER_INIT"
        },
        {
            "AR_CACHEDRESOLVER_ENV_EXPOSE_RELATIVE_PATH_IDENTIFIERS=1": 1
        }
    ]
}